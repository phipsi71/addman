<%- model_class = Mailgroup -%>


<col width="5%"  /> <!-- id  			 -->
<col width="15%" /> <!-- name      -->
<col width="5%"  /> <!-- zahnrad   -->
<col width="15%" /> <!-- trialcode -->
<col width="15%" /> <!-- role      -->
<col width="15%" /> <!-- country   -->
<col width="10%" /> <!-- count     -->
<col width="20%" /> <!-- actions   -->

<thead>
  <tr>
    <th class="dim"><%= model_class.human_attribute_name(:id) %></th>
    <th class="loaderlink"><%= sortable("name") %></th>
    <th><%= model_class.human_attribute_name(:type) %></th>
    <th><%= model_class.human_attribute_name(:trialcode) %></th>
    <th><%= model_class.human_attribute_name(:role) %></th>
    <th><%= model_class.human_attribute_name(:country) %></th>
    <th class="loaderlink"><%= sortable("user_count", "# Users") %></th>
    <th><%=t '.actions', default: t("helpers.actions") %></th>
  </tr>
</thead>
